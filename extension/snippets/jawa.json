{
  "Print to console": {
    "prefix": "cithak",
    "body": [
      "cithak(\"$1\")"
    ],
    "description": "Print to console"
  },
  "If statement": {
    "prefix": "yen",
    "body": [
      "yen ($1) {",
      "    $0",
      "}"
    ],
    "description": "If statement"
  },
  "If-Else statement": {
    "prefix": "yen-liyane",
    "body": [
      "yen ($1) {",
      "    $2",
      "} liyane yen ($3) {",
      "    $0",
      "}"
    ],
    "description": "If-Else statement"
  },
  "For loop": {
    "prefix": "ngulang",
    "body": [
      "ngulang (owahi $1 = 0; $1 < $2; $1++) {",
      "    $0",
      "}"
    ],
    "description": "For loop"
  },
  "Variable declaration": {
    "prefix": "owahi",
    "body": [
      "owahi $1 = $2"
    ],
    "description": "Variable declaration"
  },
  "Open file for reading": {
    "prefix": "file-waca",
    "body": [
      "JawaFile* $1 = file_buka(\"$2\", \"waca\")",
      "yen ($1) {",
      "    $0",
      "    file_tutup($1)",
      "}"
    ],
    "description": "Open file for reading"
  },
  "Open file for writing": {
    "prefix": "file-tulis",
    "body": [
      "JawaFile* $1 = file_buka(\"$2\", \"tulis\")",
      "yen ($1) {",
      "    file_tulis($1, \"$3\")",
      "    $0",
      "    file_tutup($1)",
      "}"
    ],
    "description": "Open file for writing"
  },
  "Read entire file": {
    "prefix": "file-waca-kabeh",
    "body": [
      "JawaFile* $1 = file_buka(\"$2\", \"waca\")",
      "yen ($1) {",
      "    char* $3 = file_waca_kabeh($1)",
      "    cithak($3)",
      "    $0",
      "    file_tutup($1)",
      "}"
    ],
    "description": "Read entire file"
  },
  "Check if file exists": {
    "prefix": "file-ada",
    "body": [
      "owahi $1 = file_ada(\"$2\")",
      "yen ($1) {",
      "    $0",
      "}"
    ],
    "description": "Check if file exists"
  }
}
